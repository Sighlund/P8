<AnchorPane prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: D3D3D3;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HistoryController">
   <children>
      <ImageView fx:id="vegetablesImageView" fitHeight="211.0" fitWidth="801.0" layoutX="-5.0" layoutY="-9.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="https://raw.githubusercontent.com/Sighlund/P8/main/src/main/resources/img/Vegetables.jpg" />
         </image>
         <viewport>
            <Rectangle2D height="300.0" width="1500.0" />
         </viewport>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#c2c2c280" height="48.0" layoutX="222.0" layoutY="46.0" stroke="TRANSPARENT" strokeType="INSIDE" width="338.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="222.0" layoutY="45.0" prefHeight="48.0" prefWidth="338.0" text="HISTORIK" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="SansSerif Bold" size="18.0" />
         </font>
      </Label>
      <Button layoutX="693.0" layoutY="200.0" mnemonicParsing="false" onAction="#sendToCalculationPageControllerAction" prefHeight="64.0" prefWidth="97.0" style="-fx-background-color: #27AE60;" text="Sammenlign" textFill="WHITE" />
      <Button fx:id="START" layoutY="-1.0" mnemonicParsing="false" onAction="#switchToSceneFrontPage" style="-fx-background-color: dodgerblue;" text="Menu" />
      <TableView fx:id="tableView" layoutX="24.0" layoutY="248.0" onMouseClicked="#getSelected" prefHeight="279.0" prefWidth="648.0">
         <columns>
            <TableColumn fx:id="year" prefWidth="75.0" text="År" />
            <TableColumn fx:id="quarter" prefWidth="75.0" text="Kvartal" />
            <TableColumn fx:id="kitchen" text="Enhed" />
            <TableColumn fx:id="id" prefWidth="75.0" text="Id" />
         </columns>
      </TableView>
      <ChoiceBox fx:id="choiceboxChooseKitchenHis" layoutX="24.0" layoutY="204.0" prefHeight="26.0" prefWidth="114.0" />
      <ChoiceBox fx:id="choiceboxChooseYearHis" layoutX="157.0" layoutY="204.0" prefHeight="26.0" prefWidth="114.0" />
      <ChoiceBox fx:id="choiceboxChooseQuarterHis" layoutX="287.0" layoutY="204.0" prefWidth="150.0" />
      <Label layoutX="24.0" layoutY="182.0" text="Vælg Afdeling">
         <font>
            <Font name="SansSerif Regular" size="12.0" />
         </font>
      </Label>
      <Label layoutX="157.0" layoutY="181.0" text="Vælg år" />
      <Label layoutX="287.0" layoutY="181.0" text="Vælg kvartal" />
   </children>
</AnchorPane>
